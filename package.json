{
  "name": "is-cyclic",
  "version": "0.1.2",
  "description": "A simple library to detect cyclic object references in JavaScript.",
  "source": "src/index.js",
  "main": "dist/is-cyclic.js",
  "module": "dist/is-cyclic.m.js",
  "esmodule": "dist/is-cyclic.modern.js",
  "scripts": {
    "lint": "xo",
    "fix": "xo --fix",
    "test": "xo --fix && ava",
    "build": "microbundle",
    "dev": "microbundle watch"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/levibit/is-cyclic.git"
  },
  "keywords": [
    "object",
    "cyclic",
    "circular",
    "reference"
  ],
  "author": "Levi Wong",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/levibit/is-cyclic/issues"
  },
  "homepage": "https://github.com/levibit/is-cyclic#readme",
  "files": [
    "src",
    "dist"
  ],
  "devDependencies": {
    "ava": "^3.12.1",
    "esm": "^3.2.25",
    "microbundle": "^0.12.3",
    "prettier": "^2.1.2"
  },
  "dependencies": {
    "xo": "^0.33.1"
  }
}
